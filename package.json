{
  "name": "ror",
  "version": "0.0.0",
  "main": "app.js",
  "scripts": {
    "ng": "ng",
    "start": "electron .",
    "package-win": "electron-packager . ROR --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/Setup.ico --prune=true --ignore=\"(.angular|node_modules|src/assets)\" --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"ROR\"",
    "build": "ng build",
    "dev:electron": "ng build --base-href ./ && DEBUG=true electron app.js",
    "start:electron": "ng build --base-href ./ && electron app.js",
    "dist": "electron-builder --windows",
    "package": "electron-builder -p always",
    "test": "ng test",
    "dev": "nodemon src/app.js"
  },
  "build":{
    "publish":[
      {
        "provider":"github",
        "owner":"",
        "repo":"https://github.com/Element-Tree/ROR-FrontEnd"
      }
    ]
  },
  "repository": "https://github.com/Element-Tree/ROR-FrontEnd",
  "publish": {
    "provider": "github",
    "releaseType": "release"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.0",
    "@angular/common": "^15.2.0",
    "@angular/compiler": "^15.2.0",
    "@angular/core": "^15.2.0",
    "@angular/forms": "^15.2.0",
    "@angular/platform-browser": "^15.2.0",
    "@angular/platform-browser-dynamic": "^15.2.0",
    "@angular/router": "^15.2.0",
    "@auth0/angular-jwt": "^5.2.0",
    "@videogular/ngx-videogular": "^8.0.0",
    "angular-moment": "^1.3.0",
    "crypto-js": "^4.2.0",
    "electron-log": "^5.1.1",
    "howler": "^2.2.4",
    "ng-zorro-antd": "^15.1.1",
    "ngx-cookie-service": "^15.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "update-electron-app": "^3.0.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.4",
    "@angular/cli": "~15.2.4",
    "@angular/compiler-cli": "^15.2.0",
    "@types/core-js": "^2.5.8",
    "@types/crypto-js": "^4.2.2",
    "@types/jasmine": "~4.3.0",
    "electron": "^28.2.1",
    "electron-builder": "^24.9.1",
    "electron-packager": "^17.1.2",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~4.9.4"
  }
}
